{
  "name": "tree-sitter-zetteltasken",
  "version": "0.1.0",
  "description": "Treesitter grammar for a markdown-derivative that additionally supports @tags(with values) and custom checkboxes on lists and headings.",
  "main": "index.js",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "test-corpus": "tree-sitter test",
    "generate": "tree-sitter generate"
  },
  "keywords": [
    "treesitter",
    "markdown",
    "task",
    "todo",
    "tag"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "author": "Ashok Menon",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.23.0",
    "prebuildify": "^6.0.1"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.zetteltasken",
      "injection-regex": "^zetteltasken$"
    }
  ]
}
